
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}
model User {
  id  Int @id @default(autoincrement())
  // email String @unique
  name String?
  username String @unique
  password String

  blogs Blog[]
}

model Blog{
  id  Int @id @default(autoincrement())
  authorId Int
  content String
  title String
  published Boolean @default(false)
  author User @relation(fields: [authorId], references: [id])
}